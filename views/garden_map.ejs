<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
  <link rel="stylesheet" href="./styles/index.css">
  <link rel="stylesheet" href="./styles/filter_map.css">

  <title>Garden Map - UrbaCrop</title>

  <link rel="apple-touch-icon" sizes="180x180" href="./favicon_package/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="./favicon_package/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="./favicon_package/favicon-16x16.png">
  <link rel="manifest" href="./favicon_package/site.webmanifest">
  <link rel="mask-icon" href="./favicon_package/safari-pinned-tab.svg" color="#00a300">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">
</head>

<body>
  <nav class="navbar navbar-light navbar-fixed-top" style="background-color: #e2efdd;">
    <div class="container-fluid">

      <button id="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasExample"
        aria-controls="offcanvasExample">
        <img src="images/See as list.svg">
      </button>

      <img id="topnav-logo" class="navbar-brand" src="images/leaf_logo.svg" alt="Green leaf logo">

    </div>
  </nav>

  <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasExample" aria-labelledby="offcanvasExampleLabel">
    <div class="offcanvas-header">
      <h5 class="offcanvas-title" id="offcanvasExampleLabel">UrbaCrop</h5>
      <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <div class="user-info">
        <a href="gardener_profile.html">
          <img id="user-pro-pic" src="https://via.placeholder.com/150" alt="User Profile Picture">
          <div class="name-title">
            <h3>Suaveman Jones</h3>
            <h4>Gardener</h4>
          </div>
        </a>
      </div>
      <div class=quick-nav>
        <div class="menu-option">
            <img src="images/applications.svg" alt="Applications Icon">
            <a href="#">Your Applications</a>
        </div>
        <div class="menu-option">
            <img src="images/favorites.svg" alt="Favorites Icon">
            <a href="#">Your Favorites</a>
        </div>
        <div class="menu-option">
            <img src="images/guidelines.svg" alt="Guidelines Icon">
            <a href="#">Farming Guidelines</a>
        </div>
        <div class="menu-option">
          <img src="images/about_us.svg" alt="About Us Icon">
          <a href="about_us.html">About Us</a>
        </div>
        <div class="menu-option">
            <img src="images/settings.svg" alt="Settings Icon">
            <a href="#">Settings</a>
        </div>
        <div class="menu-option">
            <img src="images/logout.svg" alt="Log out Icon">
            <a href="#">Log out</a>
        </div>
      </div>
    </div>
  </div>

  <div class="filter-menu">
    <button type="button" data-bs-toggle="modal" data-bs-target="#exampleModal">
      <img src="images/Filter.svg" alt="Filter Icon">
    </button>
    <button type="button" id="list_toggle_button">
      <img src="images/See as list.svg" alt="See as list Icon">
    </button>
  </div>

  <!-- Modal -->
  <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Filter By</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <label for="customRange2" class="form-label">Distance (1-10km):</label>
          <input type="range" class="form-range" min="0" max="5" id="customRange2">

          <div class="form-check">
            <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
            <label class="form-check-label" for="flexCheckDefault">
              Greens
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
            <label class="form-check-label" for="flexCheckChecked">
              Vegetables
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>
            <label class="form-check-label" for="flexCheckChecked">
              Flowers
            </label>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Save changes</button>
        </div>
      </div>
    </div>
  </div>

  <!--Map Result as a List-->
  <div class="accordion invisible" id="accordionPanelsStayOpenExample">
    <div class="accordion-item">
      <h2 class="accordion-header" id="panelsStayOpen-headingOne">
        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="true" aria-controls="panelsStayOpen-collapseOne">
          1. Kingscrest Park Community Garden
        </button>
      </h2>
      <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingOne">
        <div class="accordion-body">
          MAPID: FA081<br>
          YEAR_CREATED: 2014<br>
          MERGED_ADDRESS: 4150 Knight St, Vancouver, BC<br>
          NUMBER_OF_PLOTS: 20<br>
          JURISDICTION: Park Board<br>
          STEWARD_OR_MANAGING_ORGANIZATION: Cedar Cottage Food Network Society<br>
          PUBLIC_E_MAIL: kingcrestgarden@gmail.com<br>
        </div>
      </div>
    </div>
    <div class="accordion-item">
      <h2 class="accordion-header" id="panelsStayOpen-headingTwo">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseTwo" aria-expanded="false" aria-controls="panelsStayOpen-collapseTwo">
          2. South Vancouver Youth Centre Day Program
        </button>
      </h2>
      <div id="panelsStayOpen-collapseTwo" class="accordion-collapse collapse" aria-labelledby="panelsStayOpen-headingTwo">
        <div class="accordion-body">
          MAPID: FA125<br>
          YEAR_CREATED: 2008<br>
          MERGED_ADDRESS: 4866 Fraser St, Vancouver, BC<br>
          NUMBER_OF_PLOTS: 3<br>
          JURISDICTION: Private<br>
          STEWARD_OR_MANAGING_ORGANIZATION: South Van Youth Ctr Day Program<br>
          Geo Local Area: Kensington-Cedar Cottage<br>
        </div>
      </div>
    </div>
    <div class="accordion-item">
      <h2 class="accordion-header" id="panelsStayOpen-headingThree">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseThree" aria-expanded="false" aria-controls="panelsStayOpen-collapseThree">
          3. Frog Hollow Neighbourhood House Garden 1
        </button>
      </h2>
      <div id="panelsStayOpen-collapseThree" class="accordion-collapse collapse" aria-labelledby="panelsStayOpen-headingThree">
        <div class="accordion-body">
          MAPID:FA060<br>
          YEAR_CREATED: 2007<br>
          MERGED_ADDRESS: 2703 E 1st  Av, Vancouver, BC<br>
          NUMBER_OF_PLOTS: 5<br>
          JURISDICTION: Private
          STEWARD_OR_MANAGING_ORGANIZATION: Frog Hollow Neighbourhood House<br>
          PUBLIC_E_MAIL: eva@froghollow.bc.ca<br>
          WEBSITE: http://www.froghollow.bc.ca<br>
          Geo Local Area: Hastings-Sunrise<br>
        </div>
      </div>
    </div>
  </div>

  <div class="container-fluid" id="bottom_grid">
    <div class="row">
      <div class="col">
        <div class="col_content">
          <img src="images/Home.svg">
        </div>
      </div>
        <div class="col">
          <a href="garden_map.html">
            <div class="col_content">
              <img src="images/Discovery.svg">
            </div>
          </a>
        </div>
      <div class="col">
        <div class="col_content">
          <img src="images/Notification.svg">
        </div>
      </div>
    </div>
  </div>

  <!-- Optional JavaScript; choose one of the two! -->

  <!-- Option 1: Bootstrap Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8"
    crossorigin="anonymous"></script>
  <script src="scripts/garden_map.js"></script>

